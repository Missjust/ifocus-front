<view class="weui-cells">
  <navigator url="" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
    <view class="weui-cell__hd">
      <view class="user-info__avatar">
        <open-data type="userAvatarUrl"></open-data>
      </view>
    </view>
    <view class="weui-cell__bd">
      <view class="user-info__name">
        {{nickname}}
        <!--<open-data type="userNickName"></open-data>-->
      </view>
      <view class="font-gray-28" style="color:grey; font-size:28rpx">ID: {{openid}}</view>
      <view class="font-gray-28" style="color:grey; font-size:28rpx">签名: {{slogan}}</view>
    </view>
    <view class="weui-cell__ft weui-cell__ft_in-access"></view>
  </navigator>
</view>


<view class="weui-cells">
  <view class="weui-cell weui-cell_access" hover-class="weui-cell_active">
    <view class="weui-cell__hd">
      <!-- <image src="/images/icon_logout.png" class="icon"></image> -->
    </view>
      虚拟币：{{money}}
    <view class="weui-cell__ft weui-cell__ft_in-access"></view>
  </view>
</view>

<view class="weui-cells">
  <view class="weui-cell weui-cell_access" hover-class="weui-cell_active">
    <navigator url="../scorehistory/scorehistory">历史记录</navigator>
    <view class="weui-cell__ft weui-cell__ft_in-access"></view>
  </view>
</view>
